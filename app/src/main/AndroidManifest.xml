<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stefapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_hdpi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Stefapp">
        <activity android:name=".Add_audio"
            android:theme="@style/Theme.Stefapp.NoActionBar"/>
        <activity android:name=".SplashActivity"
            android:theme="@style/Theme.Stefapp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.LoginEmailPasswordActivity"
            android:theme="@style/Theme.Stefapp.NoActionBar"/>
        <activity android:name=".login.LogActivity"
            android:theme="@style/Theme.Stefapp.NoActionBar"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Stefapp.NoActionBar" />
    </application>

</manifest>